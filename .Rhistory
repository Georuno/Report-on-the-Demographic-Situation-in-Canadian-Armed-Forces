# Use 1 through to 151 to represent each riding
'Total' = 1:151,
# Randomly choose one of five options, with replacement, 151 times
'year' = sample(
x = c(
'Liberal',
'Labor',
'National',
'Green',
'Other'
),
size = 151,
replace = TRUE
))
simulated_CAF_member_data <-
tibble(
year =
c(1997:2020),
officers =
runif(n = 24,
min = 13000,
max = 55000),
ncm =
runif(n = 24,
min = 48000,
max = 55000),
total =
c(officers + ncm)
)
set.seed(853)
simulated_CAF_Attrition_data <-
